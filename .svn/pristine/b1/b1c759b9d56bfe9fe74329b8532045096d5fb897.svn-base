-- Table structure for ticket_package_activity
-- ----------------------------
DROP TABLE IF EXISTS `ticket_package_activity`;
CREATE TABLE `ticket_package_activity` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `rule_id` int(11) NOT NULL,
  `activity_name` varchar(25) DEFAULT NULL,
  `description` varchar(255) DEFAULT NULL,
  `status` int(11) DEFAULT NULL,
  `effective_day` int(11) DEFAULT NULL,
  `create_time` datetime NOT NULL,
  `operator` varchar(25) DEFAULT NULL,
  PRIMARY KEY (`id`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8;
