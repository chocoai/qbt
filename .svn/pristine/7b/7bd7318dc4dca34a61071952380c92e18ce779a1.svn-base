<script>
    $(function(){
        $(document).keydown(function(event){
            if(event.keyCode == 13){
                $('#orderSearch').click();
            }
        });
        $('#orderSearch').click(function(){
            var orderNumber = $("#orderNumber").val();
            if(!orderNumber ){
                $('#orderNumber').focus();
                layer.msg('请输入您的单号',{icon:0,time:1500});
                return;
            }

            $.ajax({
                url: $.baseUrl + "/official/track_order.api",
                type: "post",
                data: {
                    keyword:orderNumber
                },
                success: function(data) {
                    if(!data.datas){
                        $('.tarck-main').hide();
                        $('#orderNumber').focus();
                        layer.msg(data.msg,{icon:0,time:1500});
                        return;
                    }
                    var datas = data.datas;
                    var busnissType = datas.busnissType;
                    var orderMsg1 = [datas.number,datas.jUserName+" "+datas.jMobile+" "+datas.jCityName+" "+datas.jAddressName,datas.sendTimeSection,datas.freight,datas.insureFee];
                    var orderMsg11 = ['订单编号：','球包发自：','预约寄件时间：','订单金额：￥','保价费：￥'];
                    var orderMsg2 = [new Date(datas.createTime).Format('yyyy-MM-dd hh:mm:ss'),datas.dUserName+" "+datas.dMobile+" "+datas.dCityName+" "+datas.dAddressName,datas.deliverTime,datas.insureValue+"万元"];
                    var orderMsg22 = ['下单时间：','球包寄往：','预计到达时间：','保价金额：'];
                    $("#wuliu,#orderMsg1,#orderMsg2").children().remove();
                    for(var i=0; i<datas.waybills.length;i++){
                        var wuliuDetail = datas.waybills[i];
                        var wuliuMsg = "<li class='list-group-item list-on'><span class='font-80'>"+new Date(wuliuDetail.acceptTime).Format('yyyy-MM-dd hh:mm:ss')
                        +"</span>"+wuliuDetail.remark+wuliuDetail.acceptAddress;
                        $("#wuliu").append(wuliuMsg);
                    }

                    for(var i=0;i<orderMsg1.length;i++){
                        var orderP = "<p><span class='font-80'>"+orderMsg11[i]+"</span>"+orderMsg1[i]+"</p><br />"
                        $("#orderMsg1").append(orderP);
                    }
                    for(var i=0;i<orderMsg2.length;i++){
                        var orderP = "<p><span class='font-80'>"+orderMsg22[i]+"</span>"+orderMsg2[i]+"</p><br />"
                        $("#orderMsg2").append(orderP);
                    }
                    $("#allAmount").html("￥"+datas.totalAmount);

                    var orderStatus = 1;
                    if(datas.deliveryStatus == 7){
                        orderStatus = 3
                    }else if(datas.deliveryStatus == 3 || datas.deliveryStatus == 4 || datas.deliveryStatus == 5 || datas.deliveryStatus == 6){
                        orderStatus = 2;
                    }else if(datas.orderStatus == 3 && (datas.deliveryStatus == 1 || datas.deliveryStatus == 2)){
                        orderStatus = 1;
                    }else{
                        orderStatus = 0;
                    }
                    $('#orderStatus div').eq(orderStatus).addClass('ord-tit').siblings().removeClass('ord-tit');
                    if (!data.datas.waybills.length){
                        $(".wuliu").hide();
                    }
                    $('.tarck-main').show();
                }


            });
            $('#orderNumber').focus();
        });

    });
    Date.prototype.Format = function (fmt) {
        var o = {
            "M+": this.getMonth() + 1, //月份
            "d+": this.getDate(), //日
            "h+": this.getHours(), //小时
            "m+": this.getMinutes(), //分
            "s+": this.getSeconds(), //秒
            "q+": Math.floor((this.getMonth() + 3) / 3), //季度
            "S": this.getMilliseconds() //毫秒
        };
        if (/(y+)/.test(fmt)) fmt = fmt.replace(RegExp.$1, (this.getFullYear() + "").substr(4 - RegExp.$1.length));
        for (var k in o)
        if (new RegExp("(" + k + ")").test(fmt)) fmt = fmt.replace(RegExp.$1, (RegExp.$1.length == 1) ? (o[k]) : (("00" + o[k]).substr(("" + o[k]).length)));
        return fmt;
    };



</script>
