/*------------animate-----------------*/
.fade-enter-active, .fade-leave-active {
  transition: opacity .3s ease;
}

.fade-enter, .fade-leave-active {
  opacity: 0;
}

.bounce-enter-active {
  -webkit-animation: bounceInLeft 0.3s;
  animation: bounceInLeft 0.3s;
}

.bounce-leave-active {
  -webkit-animation: bounceOutRight 0.3s;
  animation: bounceOutRight 0.3s;
}

.fadeInLeft-enter-active {
  -webkit-animation: bounceInLeft 0.3s;
  animation: bounceInLeft 0.3s;
}

.fadeInLeft-leave-active, .fadeOutRight-enter-active {
  opacity: 0;
}

.fadeOutRight-leave-active {
  z-index: 2;
  -webkit-animation: bounceOutRight 0.3s;
  animation: bounceOutRight 0.3s;
}

@-webkit-keyframes bounceInLeft {
  0% {
    opacity: 0;
    -webkit-transform: translate3d(100%, 0, 0);
    transform: translate3d(100%, 0, 0);
  }
  100% {
    opacity: 1;
    -webkit-transform: translate3d(0, 0, 0);
    transform: translate3d(0, 0, 0);
  }
}

@keyframes bounceInLeft {
  0% {
    opacity: 0;
    -webkit-transform: translate3d(100%, 0, 0);
    transform: translate3d(100%, 0, 0);
  }
  100% {
    opacity: 1;
    -webkit-transform: translate3d(0, 0, 0);
    transform: translate3d(0, 0, 0);
  }
}

@-webkit-keyframes bounceOutRight {
  0% {
    -webkit-transform: translate3d(0, 0, 0);
    transform: translate3d(0, 0, 0);
  }

  to {
    -webkit-transform: translate3d(100%, 0, 0);
    transform: translate3d(100%, 0, 0);
  }
}

@keyframes bounceOutRight {
  0% {
    -webkit-transform: translate3d(0, 0, 0);
    transform: translate3d(0, 0, 0);
  }

  to {
    -webkit-transform: translate3d(100%, 0, 0);
    transform: translate3d(100%, 0, 0);
  }
}

