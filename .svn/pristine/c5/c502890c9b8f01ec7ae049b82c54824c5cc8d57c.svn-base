.festivals .ticket-content ul li:before {
    content: "";
    display: block;
    width: 8px;
    height: 6px;
    border: 2px solid rgb(21, 202, 95);
    position: absolute;
    left: 15px;
    top: -4px;
    border-radius: 50%;
    background-color: rgb(255, 255, 255)
}

.festivals .ticket-content ul li:after {
    content: "";
    display: block;
    width: 8px;
    height: 6px;
    border: 2px solid rgb(21, 202, 95);
    position: absolute;
    left: 15px;
    top: 64px;
    border-radius: 50%;
    background-color: rgb(255, 255, 255)
}

.festivals .ticket-content ul li .money {
    font-size: 20px;
    color: #ff5400;
    margin-top: 10px;
}

.festivals .ticket-content ul li .activity {
