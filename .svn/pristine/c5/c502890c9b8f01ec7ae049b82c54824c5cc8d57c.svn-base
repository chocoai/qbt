}

.festivals .activate {
    border-bottom: 1px solid #D9D9D9;
}

.festivals .activate>p {
    font-size: 15px;
    text-align: center;
    margin-top: 15px;
}

.festivals .activate>p .cancel-activate {
    background-color: #15ca5f;
    display: inline-block;
    padding: 3px;
    border-radius: 3px;
    color: #fff;
    font-size: 13px;
}

.festivals .activate p:last-of-type {
    color: #a9a9a9;
    line-height: 25px;
    margin-top: 2px;
}

.festivals .voucherProperty {
