package com.qbt.persistent.entity;

import java.util.Date;
import java.util.List;

public class ActivityTicketPackage {
	
	private int id;
	private String activityName;
	private Date expirationTime;
	private int effectiveDay;
	private Date activateTime;
	private String activateUser;
	private String activatePhoneNumber;
	private String code;
	private int codeStatus;
	private int status;
	private int activeStatus;
	private float price;
	private List<UserPackageTicket> tickets;
	public int getId() {
		return id;
	}
	public void setId(int id) {
		this.id = id;
	}
	public String getActivityName() {
		return activityName;
	}
	public void setActivityName(String activityName) {
		this.activityName = activityName;
	}
	public Date getExpirationTime() {
		return expirationTime;
	}
	public void setExpirationTime(Date expirationTime) {
		this.expirationTime = expirationTime;
	}
	public int getEffectiveDay() {
		return effectiveDay;
	}
	public void setEffectiveDay(int effectiveDay) {
		this.effectiveDay = effectiveDay;
	}
	public Date getActivateTime() {
		return activateTime;
	}
	public void setActivateTime(Date activateTime) {
		this.activateTime = activateTime;
	}
	public String getActivateUser() {
		return activateUser;
	}
	public void setActivateUser(String activateUser) {
		this.activateUser = activateUser;
	}
	public String getActivatePhoneNumber() {
		return activatePhoneNumber;
	}
	public void setActivatePhoneNumber(String activatePhoneNumber) {
		this.activatePhoneNumber = activatePhoneNumber;
	}
	public String getCode() {
		return code;
	}
	public void setCode(String code) {
		this.code = code;
	}
	public int getCodeStatus() {
		return codeStatus;
	}
	public void setCodeStatus(int codeStatus) {
		this.codeStatus = codeStatus;
	}
	public int getStatus() {
		return status;
	}
	public void setStatus(int status) {
		this.status = status;
	}
	public int getActiveStatus() {
		return activeStatus;
	}
	public void setActiveStatus(int activeStatus) {
		this.activeStatus = activeStatus;
	}
	public List<UserPackageTicket> getTickets() {
		return tickets;
	}
	public void setTickets(List<UserPackageTicket> tickets) {
		this.tickets = tickets;
	}
	public float getPrice() {
		return price;
	}
	public void setPrice(float price) {
		this.price = price;
	}
	

}
