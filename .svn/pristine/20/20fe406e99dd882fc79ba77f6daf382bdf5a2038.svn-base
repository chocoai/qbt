<!DOCTYPE html>
<html lang="en" ng-app="app">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <link href="/assets/bootstrap/dist/css/bootstrap.min.css" rel="stylesheet">
    <link href="/assets/admin/css/sb-admin-2.css" rel="stylesheet">
    <link href="/assets/font-awesome/css/font-awesome.min.css" rel="stylesheet" type="text/css">
    <link href="/assets/bootstrap-datepicker/bootstrap-datepicker.min.css" rel="stylesheet" type="text/css">
    <link href="{{{/css/main.min.css}}}" rel="stylesheet" type="text/css" >
    <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
        <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
        <script src="https://oss.maxcdn.com/libs/respond.js/1.4.2/respond.min.js"></script>
        <![endif]-->
    </head>

    <body ng-controller="systemlogCtrl">
        <div class="container containerWidthAuto">
            <div class="row">
                <div class="col-sm-12">
                    <div class="panel panel-default mt10">
                        <div class="panel-heading">
                            <h3 class="panel-title">日志
                            </h3>
                        </div>
                        <div class="panel-body">
                            <div class="row">
                                <div class="col-sm-12">
                                    <form class="form-inline applyCheckSearch">
                                        <div class="form-group marginRight5">
                                            <label>开始时间：</label>
                                            <input type="text" class="form-control date-picker" placeholder="选择开始时间" ng-model="startTime" readonly>
                                        </div>
                                        <div class="form-group marginRight5">
                                            <label>结束时间：</label>
                                            <input type="text" class="form-control date-picker" placeholder="选择结束时间" ng-model="endTime" readonly>
                                        </div>
                                        <div class="form-group">
                                            <button id="search" class="btn btn-primary" ng-click="getList()">搜索</button>
                                        </div>
                                    </form>
                                </div>
                            </div>
                            <ul class="timeline">
                                <div ng-repeat="systemlog in systemlogs | orderBy:'createTime':true">
                                    <li class="time-label">
                                        <span>{{systemlog.type == 1 ? '微信用户' : 'boss用户' }}</span>
                                        <span>{{systemlog.status == 1 ? '未处理' : '已处理'}}</span>
                                        <span>{{systemlog.createTime | date : 'yyyy-MM-dd HH:mm:ss'}}</span>
                                    </li>
                                    <li>
                                        <i class="fa fa-envelope bg-blue"></i>
                                        <div class="timeline-item">
                                            <h3 class="timeline-header">
                                                {{systemlog.memo}}
                                            </h3>
                                        </div>
                                    </li>
                                </div>
                            </ul>
                        </div>
                        <pagination page="page" change="getList(page)"></pagination>
                    </div>
                </div>
            </div>
        </div>

<script src="/assets/jquery/dist/jquery.min.js"></script>
<script src="/assets/jquery.nicescroll/jquery.nicescroll.min.js"></script>
<script src="/assets/angular/angular.min.js"></script>
<script src="/assets/bootstrap/dist/js/bootstrap.min.js"></script>
<script src="/assets/layer/layer.js"></script>
<script src="/assets/bootstrap-datepicker/bootstrap-datepicker.min.js"></script>
<script src="{{{/js/app.js}}}"></script>
<script src="{{{/js/systemlogCtrl.js}}}"></script>
</body>
</html>
