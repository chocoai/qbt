<script src="<%=static_url%>/js/citySet.js"></script>
<script>

$(function(){
    var jAddressId = '';
    var dAddressId = '';
    var jThree = 0;
    var dThree = 0;
    var consigned_time = '';

    $('#city').val('');
    $('#city-2').val('');

    $(".book-name").each(function(index,ele) {
        $(ele).click(function(e){
            $(ele).parent().toggleClass("open02");
			e.stopPropagation();
		});
    });
    //选择城市
    $("#city").click(function (e) {
        SelCity(this,e);
        $('#_citys0 a').on('click',function(){
            jAddressId = $(this).attr('data-id');
            jThree = 1;
            $('#_citys1 a').on('click',function(){
                jAddressId = $(this).attr('data-id');
                jThree = 2;
                $('#_citys2 a').on('click',function(){
                    jAddressId = $(this).attr('data-id');
                    jThree = 3;
                });
            });
        });
    });
    $("#city-2").click(function (e) {
        SelCity(this,e);
        $('#_citys0 a').on('click',function(){
            dAddressId = $(this).attr('data-id');
            dThree = 1;
            $('#_citys1 a').on('click',function(){
                dAddressId = $(this).attr('data-id');
                dThree = 2;
                $('#_citys2 a').on('click',function(){
                    dAddressId = $(this).attr('data-id');
                    dThree = 3;
                });
            });
        });
    });

    //运费测算
    $('#priceSearch').click(function(){
        var consigned_time = $('.book-name time').html().substring(0,16)+':00';
        if(!jThree || jThree != 3 || !dThree || dThree != 3 || consigned_time == '请选择时间...:00'){
            $("#alert-box").fadeIn().delay(1800).fadeOut();
            return false;
        }
        var addrMsg = {
            jAddr : $('#city').val(),
            dAddr : $('#city-2').val()
        };
        var searchMsg = {
             consigned_time : consigned_time,
             src_area_id : jAddressId,
             dest_area_id :dAddressId,
             src_address : '全部',
             dest_address : '全部',
             business_type : 0
        };
        $.ajax({
            url:  $.baseUrl + "/comm/v2/query_sf_recive_time.api",
            type: "post",
            contentType:'application/json',
            data: JSON.stringify(searchMsg),
            success: function(data) {
                var stat = data.datas;
                var sTime = $("#startTime");
                sTime.children().remove();
                var timeText = ["<div class='col-md-4 ","'><div class='al-box'><br /><br />预计","<br /><br />","<span class='font-green'>￥","</span></div></div>"];
                if(!stat){
                    $('#remarkShow').hide();
                    $('#msgShow h3').html(data.msg);
                    $('#msgShow').show(1000);
                }
                else if(stat.length == 1){
                    var html="<span style='color:#808080'><s>原价:"+stat[0].origin_price+"</s>&nbsp;&nbsp;&nbsp;</span>";
                    !(stat[0].origin_price>stat[0].sf_price) && (html='');

                    sTime.append(timeText[0]+"col-md-offset-4"+timeText[1]+stat[0].deliver_time.split(',')[0]+timeText[2]+html+timeText[3]+stat[0].sf_price+timeText[4]);
                }
                else if(stat.length == 2){
                    var html1="<span style='color:#808080'><s>原价:"+stat[0].origin_price+"</s>&nbsp;&nbsp;&nbsp;</span>";
                    var html2="<span style='color:#808080'><s>原价:"+stat[1].origin_price+"</s>&nbsp;&nbsp;&nbsp;</span>";
                    !(stat[0].origin_price>stat[0].sf_price) && (html1='');
                    !(stat[1].origin_price>stat[1].sf_price) && (html2='');

                    sTime.append(timeText[0]+"col-md-offset-2"+timeText[1]+stat[0].deliver_time.split(',')[0]+timeText[2]+html1+timeText[3]+stat[0].sf_price+timeText[4]);
                    sTime.append(timeText[0]+timeText[1]+stat[1].deliver_time.split(',')[0]+timeText[2]+html2+timeText[3]+stat[1].sf_price+timeText[4]);
                }
                else{
                    var html1="<span style='color:#808080'><s>原价:"+stat[0].origin_price+"</s>&nbsp;&nbsp;&nbsp;</span>";
                    var html2="<span style='color:#808080'><s>原价:"+stat[1].origin_price+"</s>&nbsp;&nbsp;&nbsp;</span>";
                    var html3="<span style='color:#808080'><s>原价:"+stat[2].origin_price+"</s>&nbsp;&nbsp;&nbsp;</span>";
                    !(stat[0].origin_price>stat[0].sf_price) && (html1='');
                    !(stat[1].origin_price>stat[1].sf_price) && (html2='');
                    !(stat[2].origin_price>stat[2].sf_price) && (html3='');

                    sTime.append(timeText[0]+timeText[1]+stat[0].deliver_time.split(',')[0]+timeText[2]+html1+timeText[3]+stat[0].sf_price+timeText[4]);
                    sTime.append(timeText[0]+timeText[1]+stat[1].deliver_time.split(',')[0]+timeText[2]+html2+timeText[3]+stat[1].sf_price+timeText[4]);
                    sTime.append(timeText[0]+timeText[1]+stat[2].deliver_time.split(',')[0]+timeText[2]+html3+timeText[3]+stat[2].sf_price+timeText[4]);
                }
                if(stat){
                    $('#toDownOrder').click(function(){
                        window.location.href = 'order.html?jAddrId='
                                             + searchMsg.src_area_id
                                             + '&jAddr=' + addrMsg.jAddr
                                             + '&dAddrId='
                                             + searchMsg.dest_area_id
                                             + '&dAddr=' + addrMsg.dAddr
                                             + '&time=' + searchMsg.consigned_time;
                    });
                    $('#msgShow').hide();
                    $('#remarkShow').show();
                }
            }
        });
    });

    //选择时间
    $.ajax({
        url: $.baseUrl + "/index/time.api",
        type: "get",
        data: {
            day: '',
            prefix: '',
            items:''
        },
        success: function(data) {
          for(var i=0;i<data.datas.length;i++){
              var dDay = data.datas[i].day;
              var dayTime = data.datas[i].items
              $(".book-time #dDay").append("<a value='"+dayTime+"'>"+dDay+"</a>");
          }
          $(".book-time #dDay a").on("click",function(){
              $(".book-time #dayTime").children().remove();
              var w = $(this).attr('value');
              var day = $(this).html();
              var array = w.split (",");
              for(i=0; i<array.length;i++){
                  $(".book-time #dayTime").append("<a value='"+day+"'>"+array[i]+"</a>");
              }
              $(this).addClass('time-on').siblings().removeClass('time-on');
              $("#dDay").hide();
              $("#dayTime").show();
              $(".book-day").children().eq(0).removeClass("active");
              $(".book-day").children().eq(1).addClass("active");
              $("#dayTime a").on("click",function(){
                  $(".book-tab").removeClass("open02");
                  $(".book-name time").html($(this).attr('value')+' '+$(this).html());
              });
          });
       }
    });

    //tab
  	function tabs(tabTit,on,tabCon){
  		$(tabTit).children().click(function(){
  			$(this).addClass(on).siblings().removeClass(on);
  			var index = $(tabTit).children().index(this);
  			$(tabCon).children().eq(index).show().siblings().hide();
  		});
  	};
	 tabs(".tab-hd","active",".tab-bd");   
});
</script>
</body>
