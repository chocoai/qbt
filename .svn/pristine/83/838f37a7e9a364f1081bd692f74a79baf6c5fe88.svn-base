DROP TABLE IF EXISTS `ticket_package_rule_config`;
CREATE TABLE `ticket_package_rule_config` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `rule_id` int(11) NOT NULL,
  `ticket_name` varchar(25) DEFAULT NULL,
  `ticket_price` float DEFAULT NULL,
  `pay_price` float DEFAULT NULL,
  `quantity` int(11) DEFAULT NULL,
  `use_limit` int(11) DEFAULT NULL,
  `create_time` datetime NOT NULL,
  `update_time` datetime NOT NULL,
  `operator` varchar(25) DEFAULT NULL,
  PRIMARY KEY (`id`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8;
