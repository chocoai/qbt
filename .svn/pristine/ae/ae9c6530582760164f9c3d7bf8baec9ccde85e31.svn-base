package com.qbt.web.pojo.ticketPackageRule;

import java.util.List;


public class TicketPackageRuleVo{
	
	//券包規則id
	private int id;
	
	//規則名稱
	private String ruleName;
	
	//規則實際總額
	private float price;
	
	//規則狀態 1 - active; 2 - inactive
	private int status;
	
	//規則描述
	private String description;
	
	
	//是否存包服務
	private int isStorageService;
	
	//券包清單
	private List<TicketPackageRuleConfVo> ticketPackageRuleConfVolist;
	
	
	
	public List<TicketPackageRuleConfVo> getTicketPackageRuleConfVolist() {
		return ticketPackageRuleConfVolist;
	}
	
	public void setTicketPackageRuleConfVolist(
			List<TicketPackageRuleConfVo> ticketPackageRuleConfVolist) {
		this.ticketPackageRuleConfVolist = ticketPackageRuleConfVolist;
	}
	
	public int getId() {
		return id;
	}
	public void setId(int id) {
		this.id = id;
	}
	public String getRuleName() {
		return ruleName;
	}
	public void setRuleName(String ruleName) {
		this.ruleName = ruleName;
	}
	public float getPrice() {
		return price;
	}
	public void setPrice(float price) {
		this.price = price;
	}
	public int getStatus() {
		return status;
	}
	public void setStatus(int status) {
		this.status = status;
	}
	public String getDescription() {
		return description;
	}
	public void setDescription(String description) {
		this.description = description;
	}

	public int getIsStorageService() {
		return isStorageService;
	}

	public void setIsStorageService(int isStorageService) {
		this.isStorageService = isStorageService;
	}
	
	
	
	

}
