<template id="index_send">
<section class="page">
	<div style="text-align:center;background-color: #fff;padding:10px 0px;">
      	<img style="width:111px;height:33px" src="/img/logo.png">
    </div>
   	<div @click="goView(true)" class="weui-cell weui-cell_access bgWhite">
        <div class="weui-cell__hd">
            <img src="/img/send_v3.png" style="width: 40px;display: block;margin-right: 10px;">
        </div>
        <div class="weui-cell__bd">
            <a class="fontBlack" href="javascript:;">
            <p>{{send_addr.name}}    {{send_addr.mobile}}</p>
            <p>{{send_addr.addr_type == 4 ? send_addr.treePath.replace(/,/g,'') : ''}}
                <span v-if="send_addr.tabStyle==1">{{send_addr.address ? send_addr.address : '球包从哪里寄出'}}</span>
                <span v-if="send_addr.tabStyle!=1">{{send_addr.siteName ? send_addr.siteName : '球包从哪里寄出'}}</span>
            </p>
            </a>
        </div>
        <div class="weui-cell__ft"></div>
    </div>
    <div  @click="goView(false)" class="weui-cell weui-cell_access bgWhite">
        <div class="weui-cell__hd">
            <img src="/img/send1_v3.png" style="width: 40px;display: block;margin-right: 10px;">
        </div>
        <div class="weui-cell__bd">
            <a class="fontBlack" href="javascript:;">
                 <p>{{collect_addr.name}}    {{collect_addr.mobile}}</p>
                <p>{{collect_addr.addr_type == 4 ? collect_addr.treePath : ''}}
                    <span v-if="collect_addr.tabStyle==1">{{collect_addr.address ? collect_addr.address : '球包送到哪里去'}}</span>
                    <span v-if="collect_addr.tabStyle!=1">{{collect_addr.siteName ? collect_addr.siteName : '球包送到哪里去'}}</span>
            </a>
        </div>
        <div class="weui-cell__ft"></div>
    </div>
    <div class="button-sp-area" style="width:60%;margin:0 auto">
        <a style="border-radius:0;font-size:24px" @click="goSend" href="javascript:;" class="weui-btn weui-btn_plain-primary index_send_btn1">确定</a>
        <a style="border-radius:0;font-size:24px;border:2px solid #15ca5f;color: #32d673" @click="goBack" href="javascript:;" class="weui-btn weui-btn_plain-primary index_send_btn2">返回</a>
    </div>
    <footer class="footer font16" style="border:none;bottom:10px;">
        <div class="weui-flex">
            <div class="weui-flex__item borderRadius4" style="border:4px solid #fc776d;margin:0 25%;line-height: 20px;height:50px;">
                <a style="color:#fc776d" class="fontBlack" href="tel:400-609-0990">
                    <p style="margin-top: 5px;">联系客服</p>
                    <p>400-609-0990</p>
                </a>
            </div>
        </div>
   </footer>
</section>
</template>
